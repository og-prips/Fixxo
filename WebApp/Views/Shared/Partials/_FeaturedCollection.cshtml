@model List<ProductModel>

<section class="collection-grid">
    <div class="container">
        <div class="title">@ViewData["CollectionTitle"]</div>
        <div class="item-collection">
            @for (int i = 0; i < 8; i++)
            {
                <partial name="~/Views/Shared/Partials/Components/_CollectionItem.cshtml" model="Model[i]" />
            }
        </div>
    </div>
</section>